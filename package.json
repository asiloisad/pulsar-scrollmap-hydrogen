{
  "name": "scrollmap-hydrogen",
  "description": "Hydrogen breakpoint markers for scrollmap",
  "repository": "https://github.com/asiloisad/pulsar-scrollmap-hydrogen",
  "keywords": [
    "scrollmap",
    "hydrogen",
    "jupyter",
    "breakpoint",
    "marker"
  ],
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "atom": ">=1.100.0 <2.0.0"
  },
  "main": "./lib/main",
  "consumedServices": {
    "hydrogen.breakpoints": {
      "versions": {
        "0.0.1": "consumeHydrogenService"
      }
    }
  },
  "providedServices": {
    "scrollmap": {
      "versions": {
        "1.0.0": "provideScrollmap"
      }
    }
  },
  "configSchema": {
    "threshold": {
      "order": 1,
      "type": "integer",
      "title": "Threshold",
      "description": "Hide markers if count exceeds this value. Zero means no limit.",
      "minimum": 0,
      "default": 0
    }
  }
}
